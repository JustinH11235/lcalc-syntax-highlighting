{
    "name": "lcalc-syntax-highlighting",
    "displayName": "LCalc Syntax Highlighting",
    "description": "Syntax highlighting for the lambda calculus based language LCalc.",
    "version": "0.1.1",
    "icon": "images/logo.png",
    "publisher": "justinh0",
    "repository": {
        "type": "Github",
        "url": "https://github.com/JustinH11235/lcalc-syntax-highlighting"
    },
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lcalc",
            "aliases": ["LCalc", "lcalc"],
            "extensions": [".lc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lcalc",
            "scopeName": "source.lc",
            "path": "./syntaxes/lcalc.tmLanguage.json"
        }]
    }
}